<parameters>
<param key="nameBuildingType" value="multifamilyHouse" />

<!--Paramètres par défaut du découpage de parcelle -->

	<!-- Aire limite de découpage des parcelles -->
	<param key="areaParcel" value="2000" />
	<!-- Largeur minimum de la limite parcellaire donnant sur la voirie -->
	<!-- Si aucune voirie adjacente à la parcelle, le découpage sera orienté aléatoirement -->
	<param key="widParcel" value="6" />

<!--Paramètres par défaut des différents types de bâtiments simulés -->

	<!-- Largeur -->
	<param key="minlen" value="6" />
	<param key="maxlen" value="10" />
	<!-- Longueur -->
	<param key="minwid" value="7" />
	<param key="maxwid" value="10" />
	<!-- Hauteur -->
	<param key="minheight" value="7" />
	<param key="maxheight" value="14" />

	<!-- Fonction d'optimisation (pas (encore) utilisée) -->
	<param key="fctOpt" value="" />

	<!-- Si "true", l'intersection entre les cuboïdes est permise -->
	<param key="intersection" value="true" />

	<!-- Nombre de boites maximal composant un bâtiment -->
	<param key="nbCuboid" value="3" />

	<!-- Surface de plancher maximale pour un bâtiment -->
	<param key="areaMax" value="1050" />
	<!-- Surface miniale pour un bâtiment -->
	<param key="areaMin" value="120" />

	<!-- Nombre d'étage du bâtiment à partir duquel le dernier étage est en attique (si 99, ne s'applique pas) -->
	<param key="nbStoreysAttic" value="3" />

<!--Paramètres par défaut des différents logements inclus dans les bâtiments simulés -->

	<!-- Nombre minimum de logement par bâtiment -->
	<param key="minHousingUnit" value="2" />
		
	<!-- Borne de la classe de taille des logements de type T1/T2 -->
	<param key="sizeSmallDwelling" value="30-55" />
	<!-- Borne de la classe de taille des logements de type T3 -->
	<param key="sizeMidDwelling" value="60-75" />
	<!-- Borne de la classe de taille des logements de type T4 ou plus -->
	<param key="sizeLargeDwelling" value="80-120" />

	<!-- Fréquence de construction de logements de type T1/T2 -->
	<param key="freqSmallDwelling" value="0" />
	<!-- Fréquence de taille des logements de type T3 -->
	<param key="freqMidDwelling" value="0.5" />
	<!-- Fréquence de taille des logements de type T4 ou plus -->
	<param key="freqLargeDwelling" value="0.5" />

</parameters>
